{
  "name": "gitnotes-server",
  "version": "1.0.0",
  "description": "Git-controlled note-taking application",
  "private": true,
  "scripts": {
    "dev:server": "cd server && pnpm start:dev",
    "dev:client": "cd client && pnpm dev",
    "build:server": "cd server && pnpm build",
    "build:client": "cd client && pnpm build",
    "install:all": "pnpm install --prefix server && pnpm install --prefix client"
  },
  "keywords": [
    "notes",
    "git",
    "markdown"
  ],
  "author": "",
  "license": "MIT"
}

